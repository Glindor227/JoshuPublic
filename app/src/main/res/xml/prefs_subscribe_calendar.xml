<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:key="@string/sync_subscribe_calendar_key"
    android:title="@string/sync_subscribe_calendar_title">

    <SwitchPreference
        android:key="@string/sync_subscribe_calendar_switch_key"
        android:title="@string/sync_subscribe_calendar_title"
        android:summary="@string/sync_subscribe_switch_summary"
