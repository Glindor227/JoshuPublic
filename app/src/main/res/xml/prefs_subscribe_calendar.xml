        android:icon="@drawable/ic_add_folder_24dp"
        android:title="@string/sync_subscribe_calendar_button"
        android:dependency="@string/sync_subscribe_calendar_switch_key"
        android:shouldDisableView="true" />

</PreferenceScreen>